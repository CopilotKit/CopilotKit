{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboard": true,
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": [
        "*"
      ],
      "labels": ["dependencies"]
    },
    {
      "enabled": true,
      "matchPackageNames": [
        "/^@copilotkit/"
      ],
      "labels": ["copilotkit"],
      "groupName": "CopilotKit dependencies"
    }
  ]
}